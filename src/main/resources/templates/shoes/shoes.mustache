{{>layout/header}}
<script src="/test.hbs"></script>
<div class="content">
    <div class="shoesAll">
        <div class="">
            {{#shoesList}}
            <div class="shoes-content">
                <table>
                    <th></th>
                    <th></th>
                    <tr>
                        <td><img src="/images/{{mainImage}}"></td>
                        <td>
                            <p>사용중/리셀/보유(아이콘){{status}}</p>
                            <p>- {{brand}}</p>
                            <p>- {{name}}</p>
                            <p>- {{productCode}}</p>
                            <p>- size : {{size}}</p>
                            <p>- 발매가 : {{releasePrice}}원</p>
                            <p>- 판매가 : {{sellPrice}}원</p>
                            <p>- 설명 : {{content}}</p>
                            <p>- 구매일 : {{purchaseDate}}</p>
                            <p>- 구매처 : {{buy}}</p>
                        </td>
                    </tr>
                    <tr>
                        <td colspan=2>
                            {{#imageList}}
                                    <img src="/images/{{.}}" alt="신발이미지1">
                            {{/imageList}}
                        </td>
                    </tr>
                </table>
            </div>
            {{/shoesList}}

        </div>
    </div>
</div>

