{{>layout/header}}
<link href="/css/app/admin.css" rel="stylesheet">

<div class="content">
    <h2>West Brother_Admin</h2>

    <div class="accordion" id="accordionWrapper">
        <div class="accordion-item">
            <h2 class="accordion-header" id="프로필">
                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#내용1" aria-expanded="false" aria-controls="내용1">
                    메인프로필 관리
                </button>
            </h2>

            <div id="내용1" class="accordion-collapse collapse" aria-labelledby="프로필" data-bs-parent="#accordionWrapper">
                <div class="accordion-body">
                    <button type="button" class="btn btn-primary">프로필 전체보기</button>
                    <button type="button" class="btn btn-secondary">프로필 수정</button>
                </div>
            </div>

        </div>

        <div class="accordion-item">
            <h2 class="accordion-header" id="연락처">
                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#내용2" aria-expanded="false" aria-controls="내용2">
                    연락처 & SNS 관리
                </button>
            </h2>
            <div id="내용2" class="accordion-collapse collapse" aria-labelledby="연락처" data-bs-parent="#accordionWrapper">
                <div class="accordion-body">
                    <button type="button" class="btn btn-secondary">연락처 전체보기</button>
                    <button type="button" class="btn btn-secondary">연락처 생성</button>
                </div>
            </div>
        </div>

        <div class="accordion-item">
            <h2 class="accordion-header" id="이력서">
                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#내용3" aria-expanded="false" aria-controls="내용3">
                    이력서 관리
                </button>
            </h2>
            <div id="내용3" class="accordion-collapse collapse" aria-labelledby="이력서" data-bs-parent="#accordionWrapper">
                <div class="accordion-body">
                    <button type="button" class="btn btn-secondary">이력서 전체보기</button>
                    <button type="button" class="btn btn-secondary">Education 생성</button>
                    <button type="button" class="btn btn-secondary">Career 생성</button>
                    <button type="button" class="btn btn-secondary">Certificate 생성</button>
                    <br /><br />
                    <div class="btn-group" role="group" aria-label="Basic example">
                        <button type="button" class="btn btn-secondary">Project 전체보기</button>
                        <button type="button" class="btn btn-secondary">Project 생성</button>
                        <button type="button" class="btn btn-secondary">Project 상세정보 생성</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


<script>
    function movePage(url){
        location.href = url;
    }
</script>

{{>layout/footer}}